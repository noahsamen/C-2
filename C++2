#include <iostream>
#include <iomanip>
int main(){
  std::cout<<"Yo";
  std::cout<<"\nWhat up";
  std::cout<<"\n------------";
  std::cout<<"\nLOL";
  std::cout<<"\nPOP";
  std::cout<<"\n------";
  std::cout<<"\nAny new message?";
  std::cout<<"\nUnorganized table: ";
  std::cout<<"\n-----------------";
  std::cout<<"\nGray"<<" "<<"Cocks";
  std::cout<<"\nBalls"<<" "<<"Torture";
  std::cout<<"\nSausage"<<" "<<"Snap";
  std::cout<<"\nFormat the cock and balls torture: ";
  std::cout<<std::setw(10)<<"\nLastname"<<std::setw(10)<<" Firstname"<<std::setw(6)<<"Dicks: ";
  std::cout<<std::left;
  std::cout<<std::setw(10)<<"\nGray"<<std::setw(10)<<" Cocks "<<std::setw(6)<<"Dicks: ";
  std::cout<<std::setw(10)<<"\nBalls"<<std::setw(10)<<"Torture"<<std::setw(6)<<" Age: ";
  std::cout<<std::setw(10)<<"\nSausage"<<std::setw(10)<<" Snap "<<std::setw(6)<<"Age: ";
  std::cout<<"\nFormat a variable type.";
  std::cout<<std::right;
  int dicks {6};
  std::cout<<std::setw(dicks)<<"\nLastname"<<std::setw(dicks)<<" Firstname:";
  std::cout<<std::setfill('-');
  std::cout<<std::setw(dicks)<<"\nGray"<<std::setw(dicks)<<" Cocks "<<std::setw(dicks)<<" Dicks: "<<std::setw(dicks);
  std::cout<<std::setw(dicks)<<"\nBalls"<<std::setw(dicks)<<" Torture "<<std::setw(dicks)<<"Dicks: "<<dicks;
  std::cout<<std::setw(dicks)<<"\nSausage"<<std::setw(dicks)<<" Snap "<<std::setw(dicks)<<"Dicks: "<<std::setw(dicks/2)<<dicks;
  std::cout<<"\nTerminal Justified.";
  std::cout<<std::right;
  std::cout<<std::setw(90)<<"424214.1";
  std::cout<<std::setw(90)<<"8.909090";
  bool sausage {true};
  bool sausage2 {false};
  std::cout<<std::boolalpha;
  std::cout<<"\nShow the variables: "<<sausage;
  std::cout<<"\nShow the variables: "<<sausage2;
  std::cout<<std::noboolalpha;
  std::cout<<"\nNo variable:"<<sausage;
  std::cout<<"\nNo variable: "<<sausage2; 
  int cocks {-78219};
  float cocks2 {6.142};
  double cocks3 {121421412ul};
  std::cout<<std::showpos;
  std::cout<<"\nShow the postive numbers:";
  std::cout<<"\n"<<cocks;
  std::cout<<"\n"<<cocks2;
  std::cout<<"\n"<<cocks3;
  std::cout<<std::noshowpos;
  std::cout<<"\nNo positive variables:";
  std::cout<<"\n"<<cocks;
  std::cout<<"\n"<<cocks2;
  std::cout<<"\n"<<cocks3;
  std::cout<<"\n----------------";
  std::cout<<"\nHexadecimal and etc.";
  std::cout<<"\ncocks:"<<std::hex<<cocks;
  std::cout<<"\ncocks2:"<<std::oct<<cocks2;
  std::cout<<"\ncocks3:"<<std::dec<<cocks3;
  std::cout<<"\n-----------------";
  std::cout<<"\nUppercase:";
  std::cout<<std::uppercase;
  std::cout<<"\ncocks upper case:"<<std::hex<<cocks;
 std::cout<<"\n------------------";
  std::cout<<"\nscientific:";
  std::cout<<std::scientific;
  std::cout<<"\nToture: "<<cocks2;
  std::cout.unsetf(std::ios::fixed | std::ios::scientific);
  std::cout<<"\ncocks: "<<cocks;
  std::cout<<"\n-----------------";
  std::cout<<"\nPrecisions";
  cocks3 = 1.9494942;
  std::cout<<"\ncocks3 precision:"<<cocks3;
  std::cout<<"\n-----------------";
  std::cout<<std::showpoint;
  std::cout<<"\nshow cocks3 point:"<<cocks3;
  std::cout<<std::noshowpoint;
  std::cout<<"\nshow no cocks3 point:"<<cocks3;
  return 0;

}
